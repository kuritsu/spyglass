<span class="badge treeViewItem {{this.Style}}" {{did-insert this.init}}>
    <BsTooltip>
        ({{ @target.status}}%) {{@target.description}}
    </BsTooltip>
    {{#if @target.url}}
    <a href={{@target.url}} target="_blank" rel="noopener noreferrer">
        {{ this.Value }}
    </a>
    {{else}}
        {{ this.Value }}
    {{/if}}
</span>